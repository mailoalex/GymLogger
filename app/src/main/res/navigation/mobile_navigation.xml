<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.gymloger.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.gymloger.ui.map.MapFragment"
        android:label="@string/title_map" />

    <fragment
        android:id="@+id/navigation_wifi"
        android:name="com.example.gymloger.ui.wifi.WifiFragment"
        android:label="@string/title_wifi"
        tools:layout="@layout/fragment_wifi" />
</navigation>